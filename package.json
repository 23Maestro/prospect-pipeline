{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prospect-pipeline",
  "title": "Prospect Pipeline",
  "description": "Student-athlete video editing workflow automation",
  "type": "module",
  "icon": "video.png",
  "author": "jerami_singleton",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "active-tasks",
      "title": "Active Video Tasks",
      "description": "Browse and filter Notion video tasks by status",
      "mode": "view"
    },
    {
      "name": "assign-videoteam-inbox",
      "title": "Assign Video Team Inbox",
      "description": "Assign video team inbox threads to yourself",
      "mode": "view"
    },
    {
      "name": "read-videoteam-inbox",
      "title": "Read Video Team Inbox",
      "description": "Read video team inbox messages",
      "mode": "view"
    },
    {
      "name": "email-student-athletes",
      "title": "Email Student Athletes",
      "description": "Send emails to student athletes",
      "mode": "view"
    },
    {
      "name": "video-updates",
      "title": "Video Updates",
      "description": "Update video information for athletes",
      "mode": "view"
    },
    {
      "name": "generate-names",
      "title": "Generate Names",
      "description": "Generate YouTube titles, Dropbox/Google Drive folder names, and approved video titles",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@notionhq/client": "^2.2.0",
    "@raycast/api": "^1.102.7",
    "@raycast/utils": "^2.2.1",
    "@supabase/supabase-js": "^2.58.0",
    "date-fns": "^4.1.0",
    "playwright": "^1.55.1",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.1.1",
    "@types/node": "^24.9.1",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "typescript": "^4.9.5"
  },
  "preferences": [
    {
      "name": "notionToken",
      "title": "Notion Integration Token",
      "description": "Your Notion API integration token",
      "type": "password",
      "required": true
    },
    {
      "name": "supabaseUrl",
      "title": "Supabase URL",
      "description": "The URL of your Supabase project",
      "type": "textfield",
      "required": true
    },
    {
      "name": "supabaseAnonKey",
      "title": "Supabase Anon Key",
      "description": "The anonymous key for your Supabase project",
      "type": "password",
      "required": true
    }
  ],
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "lint": "eslint . --ext .ts,.tsx"
  }
}
